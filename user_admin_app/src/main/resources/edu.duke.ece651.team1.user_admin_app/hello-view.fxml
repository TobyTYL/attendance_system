<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.team1.user_admin_app.controller.HelloController">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="button_manage_professor" mnemonicParsing="false" onAction="#manageProfessorButtonClicked" text="Manage Professor Users">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Region prefHeight="40.0" prefWidth="200.0" />
            <Button fx:id="button_manage_student" mnemonicParsing="false" onAction="#manageStudentButtonClicked" text="Manage Student Users" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </center>
   <top>
      <BorderPane prefHeight="100.0" style="-fx-background-color: #0577B1;" BorderPane.alignment="CENTER">
         <center>
            <Label alignment="CENTER" contentDisplay="CENTER" text="User Admin Center" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="33.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
   </top>
</BorderPane>
