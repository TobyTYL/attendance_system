<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.team1.user_admin_app.controller.StudentControllerJavaFX">
   <children>
      <AnchorPane prefHeight="89.0" prefWidth="600.0" style="-fx-background-color: #0577B1;">
      <children>
            <Label layoutX="169.0" layoutY="25.0" text="User Admin Center" textAlignment="CENTER">
               <font>
                  <Font name="System Bold Italic" size="33.0" />
               </font></Label>
            <ImageView fitHeight="69.0" fitWidth="56.0" layoutX="52.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../icons/user-admin-svgrepo-com.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Button fx:id="button_update_student" layoutX="248.0" layoutY="200.0" mnemonicParsing="false" onAction="#updateStudentButtonClicked" text="Update Student" textAlignment="CENTER" />
      <Button fx:id="button_remove_student" layoutX="246.0" layoutY="256.0" mnemonicParsing="false" onAction="#removeStudentButtonClicked" prefHeight="17.0" prefWidth="109.0" text="Remove Student" textAlignment="CENTER" />
      <Button fx:id="button_add_student" layoutX="257.0" layoutY="137.0" mnemonicParsing="false" onAction="#addStudentButtonClicked" text="Add Student" textAlignment="CENTER" />
      <Button fx:id="button_back_to_main_view" layoutX="519.0" layoutY="342.0" mnemonicParsing="false" onAction="#backMainPageButtonClicked" text="Back" />
   </children>
</AnchorPane>
