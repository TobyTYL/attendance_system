<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="edu.duke.ece651.team1.enrollmentApp_javafx.MainController">
    <padding>
        <Insets top="50.0" right="50.0" bottom="50.0" left="50.0"/>
    </padding>
    <Button text="Manage Courses" onAction="#onManageCoursesClick" fx:id="manageCoursesButton"/>
    <Button text="Enroll Students" onAction="#onEnrollStudentsClick" fx:id="enrollStudentsButton"/>
    <Button text="Exit" onAction="#onExitClick" fx:id="exitButton"/>
</VBox>
