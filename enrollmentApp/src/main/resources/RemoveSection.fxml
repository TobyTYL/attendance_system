<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.RemoveSectionController_java">
   <top>
       <Label text="Select a Section to Remove:" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
       <Button onAction="#onReturn" styleClass="return-button" text="Return" BorderPane.alignment="BOTTOM_CENTER" />
   </bottom>
   <center>
       <Button onAction="#onRemoveSection" text="Remove Section" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
