<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.SectionController_javafx">
   <top>
         <BorderPane prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #0577B1;" BorderPane.alignment="CENTER">
            <center>
                  <Label contentDisplay="CENTER" text="Enrollment Center" textAlignment="CENTER" BorderPane.alignment="CENTER">
                     <font>
                           <Font name="System Bold Italic" size="33.0" />
                     </font>
               </Label>
            </center>
         </BorderPane>
      </top>

   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="addSectionButton" mnemonicParsing="false" onAction="#onAddSectionClick" prefHeight="29.0" prefWidth="160.0" text="Add a section" />
            <Region prefHeight="30.0" />
            <Button fx:id="updateSectionButton" mnemonicParsing="false" onAction="#onUpdateSectionClick" prefHeight="29.0" prefWidth="160.0" text="Update a section" />
            <Region layoutX="10.0" layoutY="39.0" prefHeight="30.0" />
            <Button fx:id="removeSectionButton" mnemonicParsing="false" onAction="#onRemoveSectionClick" prefHeight="29.0" prefWidth="160.0" text="Remove a section" />
            <Region layoutX="10.0" layoutY="235.0" prefHeight="30.0" />
            <Button fx:id="returnToClassButton" mnemonicParsing="false" onAction="#onReturnToClassClick" prefHeight="26.0" prefWidth="160.0" text="Return to previous menu" />
            <Region layoutX="10.0" layoutY="176.0" prefHeight="30.0" />
            <Button fx:id="updateClassNameButton" mnemonicParsing="false" onAction="#onUpdateClassNameClick" prefHeight="29.0" prefWidth="160.0" text="Update class name" />
         </children>
      </VBox>
   </center>
</BorderPane>
