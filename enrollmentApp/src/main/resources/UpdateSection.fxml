<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.UpdateSectionController_javafx" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" spacing="10" alignment="CENTER">
    <Label text="Select a Section to Update:" />
    <ListView fx:id="sectionListView" />
    <ComboBox fx:id="professorComboBox" promptText="Assign New Professor" />
    <Button text="Update Section" onAction="#onUpdateSection" />
    <Button text="Return" onAction="#onReturn" styleClass="return-button" />
</VBox>
