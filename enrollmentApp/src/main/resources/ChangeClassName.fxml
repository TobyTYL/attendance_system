<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.ChangeClassNameController_javafx" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" spacing="10" alignment="CENTER">
    <Label text="Current Class Name:" />
    <Label fx:id="currentClassNameLabel" />
    <TextField fx:id="newClassNameField" promptText="Enter new class name" />
    <Button text="Update Class Name" onAction="#onChangeClassName" />
    <Button text="Return" onAction="#onReturn" styleClass="return-button" />
</VBox>
