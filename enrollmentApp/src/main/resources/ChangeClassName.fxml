<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.ChangeClassNameController_javafx">
   <center>
      <VBox alignment="CENTER" spacing="10">
          <Label text="Current Class Name:" />
          <Label fx:id="currentClassNameLabel" />
          <TextField fx:id="newClassNameField" promptText="Enter new class name" />
          <Button onAction="#onChangeClassName" text="Update Class Name" />
          <Button onAction="#onReturn" styleClass="return-button" text="Return" />
      </VBox>
   </center>
</BorderPane>
