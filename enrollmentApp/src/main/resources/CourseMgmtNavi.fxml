<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.CourseMgmtNaviController_javafx">
   <top>
      <BorderPane prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #0577B1;" BorderPane.alignment="CENTER">
         <center>
                <Label contentDisplay="CENTER" text="Enrollment Center" textAlignment="CENTER" BorderPane.alignment="CENTER">
                    <font>
                        <Font name="System Bold Italic" size="33.0" />
                    </font>
            </Label>
         </center>
      </BorderPane>
   </top>

   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="createClassButton" mnemonicParsing="false" onAction="#onCreateClassClick" prefHeight="29.0" prefWidth="160.0" text="Create a new class" />
            <Region prefHeight="30.0" />
            <Button fx:id="updateClassButton" mnemonicParsing="false" onAction="#onUpdateClassClick" prefHeight="29.0" prefWidth="160.0" text="Update a class" />
            <Region layoutX="10.0" layoutY="168.0" prefHeight="30.0" />
            <Button fx:id="removeClassButton" mnemonicParsing="false" onAction="#onRemoveClassClick" prefHeight="29.0" prefWidth="160.0" text="Remove a class" />
            <Region layoutX="10.0" layoutY="153.0" prefHeight="30.0" />
            <Button fx:id="returnButton" mnemonicParsing="false" onAction="#onReturnClick" text="Back to main menu" />
         </children>
      </VBox>
   </center>
</BorderPane>
