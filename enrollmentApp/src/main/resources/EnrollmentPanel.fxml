<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.EnrollmentPanelController_javafx">
   <center>
      <VBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
          <padding>
              <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
          </padding>
          <Button fx:id="manualEnrollButton" onAction="#onManualEnrollClick" text="Manual Enroll" />
          <Button fx:id="batchEnrollButton" onAction="#onBatchEnrollClick" text="Batch Enroll with CSV" />
          <Button fx:id="returnButton" onAction="#onReturnClick" text="Return to Main Menu" />
      </VBox>
   </center>
   <top>
      <Label text="Enroll  Student Center" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets top="50.0" />
         </BorderPane.margin>
      </Label>
   </top>
</BorderPane>
