<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="edu.duke.ece651.team1.enrollmentApp.controller.EnrollmentPanelController_javafx">
    <padding>
        <Insets top="50.0" right="50.0" bottom="50.0" left="50.0"/>
    </padding>
    <Button text="Manual Enroll" onAction="#onManualEnrollClick" fx:id="manualEnrollButton"/>
    <Button text="Batch Enroll with CSV" onAction="#onBatchEnrollClick" fx:id="batchEnrollButton"/>
    <Button text="Return to Main Menu" onAction="#onReturnClick" fx:id="returnButton"/>
</VBox>
