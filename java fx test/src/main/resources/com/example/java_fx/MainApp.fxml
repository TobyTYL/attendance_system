<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Pos?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.java_fx.MainAppController">
    <padding>
        <Insets top="50.0" right="50.0" bottom="50.0" left="50.0"/>
    </padding>
    <Button text="Manage Students" onAction="#onManageStudentsClick" fx:id="manageStudentsButton"/>
    <Button text="Manage Professors" onAction="#onManageProfessorsClick" fx:id="manageProfessorsButton"/>
    <Button text="Exit" onAction="#onExitClick" fx:id="exitButton"/>
</VBox>
